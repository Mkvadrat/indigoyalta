# Permanent URL redirect - generated by www.rapidtables.com
#Redirect 301 /dvuhkomnatnaja-kvartira-yalta /dvuhkomnatnye-kvartiry-v-yalte

#Редирект
RewriteRule ^odnokomnatnaja-kvartira-yalta$ http://indigo-yalta.com/kvartiry-v-yalte-odnokomnatnye/ [L,R=301]

RewriteCond %{HTTP_HOST} ^www\.(.*)$ [NC]
RewriteRule ^(.*)$ http://%1/$1 [R=301,L]
RewriteRule ^(.*)(/)$ $1 [L,R=301]
RewriteCond %{REQUEST_URI} ^system.*
RewriteRule ^(.*)$ /index.php?/$1 [L]
	
Options +FollowSymlinks
# Turn mod_rewrite on
RewriteEngine On
RewriteBase /

 <IfModule mod_php5.c>
   php_value short_open_tag 1
 </IfModule>

RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^(.*)$ index.php?$1 [L,QSA]

RewriteCond %{THE_REQUEST} ^[A-Z]{3,}\s(.*)/index\.php [NC]
RewriteRule ^ /%1 [R=301,L]

